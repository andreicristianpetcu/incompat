{
  "name": "incompat",
  "version": "1.1.1",
  "description": "Incompat is a companion for Webcompat.com. It helps track compatibility bugs for sites that you build or use every day.",
  "scripts": {
    "dev": "./node_modules/webextension-toolbox/bin/webextension-toolbox dev firefox",
    "build": "./node_modules/webextension-toolbox/bin/webextension-toolbox build -m firefox ; ./node_modules/webextension-toolbox/bin/webextension-toolbox build -m chrome"
  },
  "devDependencies": {
    "webextension-toolbox": "latest"
  }
}
